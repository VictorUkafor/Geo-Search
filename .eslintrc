{
    "parser": "babel-eslint",
    "extends": ["airbnb"],
    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module",
        "allowImportExportEverywhere": true
    },
    "env": {
        "browser" : true,
        "node" : true,
        "es6" : true
    },
    "rules": {
        "import/prefer-default-export": "off"
   }
}